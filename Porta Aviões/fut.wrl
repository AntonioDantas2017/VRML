ROUTE time_ball.fraction_changed TO interpolate_ball.set_fraction
ROUTE interpolate_ball.value_changed TO Ball.set_translation

DEF anim Script {
  eventIn SFBool clicked
  eventIn SFBool clicked_human
  eventIn SFFloat fraction
  eventIn SFBool deactivate_human
  eventIn SFBool deactivate_ball
  field SFBool human_active FALSE
  field SFBool ball_active FALSE
  field SFBool shoot FALSE
  eventOut SFBool enable_human
  eventOut SFBool enable_ball
  eventOut SFTime startTime_human
  eventOut SFTime startTime_ball
  field SFVec3f pos2 12.8 .35 -2.58
  field SFVec3f pos1 .6 .135 .6
  eventOut SFVec3f pos_ball

  url "javascript:

    function clicked(value,when) {
        if (human_active == 0) {
          if (value == 0) {
            pos_ball = pos1;
            human_active = 1;
            startTime_human = when;
            enable_human = 1;
          }
        }
    }

    function clicked_human(value,when) {
        if (human_active == 0) {
          if (value == 0) {
            pos_ball = pos1;
            human_active = 1;
            startTime_human = when;
            enable_human = 1;
          }
        }
    }

    function fraction(value,when) {
        if (shoot == 0) {
          if (value > 0.7) {
            shoot = 1;
            ball_active = 1;
            startTime_ball = when;
            enable_ball = 1;
          }
        }
    }

    function deactivate_ball(value) {
        if (value == 0) {
                  enable_ball = 0;
                  ball_active = 0;
        }
    }

    function deactivate_human(value) {
      if (value == 0) {
        shoot = 0;
        enable_human = 0;
        human_active = 0;
      }
    }
  "
}

ROUTE start_kick.isActive TO anim.clicked
ROUTE touch.isActive TO anim.clicked_human
ROUTE anim.startTime_human TO time_calcio.set_startTime
ROUTE anim.enable_human TO time_calcio.enabled
ROUTE time_calcio.fraction_changed TO anim.fraction
ROUTE anim.startTime_ball TO time_ball.set_startTime
ROUTE anim.enable_ball TO time_ball.enabled
ROUTE time_calcio.isActive TO anim.deactivate_human
ROUTE time_ball.isActive TO anim.deactivate_ball
ROUTE anim.pos_ball TO Ball.set_translation
        


